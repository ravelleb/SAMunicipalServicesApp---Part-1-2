<Window x:Class="SAMunicipalServicesApp.EventsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Events and Announcements" Height="450" Width="600">
    <Grid Background="#f3f4f6">
        <StackPanel Margin="20,20,20,50">
            <!--Events and Announcements -->
            <TextBlock Text="Local Events and Announcements (Coming Soon)" 
                       FontSize="18" 
                       FontWeight="Bold" 
                       Foreground="#005B9A" 
                       HorizontalAlignment="Center" 
                       VerticalAlignment="Center"
                       Margin="0,0,0,20"/>

        </StackPanel>
        <Button Content="Back to Main Menu" 
        Click="btnBack_Click" 
        Height="30" 
        Width="150"
        Background="#005B9A" 
        Foreground="White"
        FontWeight="Bold"
        HorizontalAlignment="Center"
        VerticalAlignment="Top"
        Margin="0,389,0,0"
        BorderBrush="Transparent"
        BorderThickness="0"
        Padding="10"/>
    </Grid>
</Window>
